{
    "env": {
    "browser": true,
    "es2021": true,
    "node": true
    },
    "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
    "prettier/prettier": 2,
    "no-console": 1,
    "quotes": ["error", "single"],
    "no-var": "error",
    "prefer-const": "error"
    }
}
